#include<stdio.h>
void main ()
{
              int length,width,area,perimeter;
              printf("enter value of length");
              scanf("%d",&length);
              printf("enter value of width");
              scanf("%d",&width);
              area=length*width;
              perimeter=2*(length+width);
              printf("area of the rectangle=%d\n",area);
              printf("perimeter of the rectangle=%d/n",perimeter);
}
