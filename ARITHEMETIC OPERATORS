#include<stdio.h>
void main ()
{
              int a,b,result;
              float div;
              printf("enter value of a");
              scanf("%d",&a);
              printf("enter value of b");
              scanf("%d",b);
              result=a+b;
              printf("sum of %d and %d=%d\n",a,b,result);
              printf("difference of %d and %d=%d\n",a,b,a-b);
              printf("product of the %d and %d=%d\n",a,b,a*b);
              div=a/b;
              printf("division of %d and %d=%f\n",a,b,div);
}
